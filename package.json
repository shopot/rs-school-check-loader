{
  "name": "vite-react-typescript",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Vite React TypeScript boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shopot/vite-react-typescript"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint ./src --ext ts,tsx --fix",
    "preview": "vite preview",
    "prepare": "husky install",
    "lintstaged": "lint-staged --relative"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write -config .prettierrc"
    ]
  },
  "dependencies": {
    "@types/compose-function": "^0.0.30",
    "compose-function": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.8"
  }
}
